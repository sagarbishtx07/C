<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.C" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/toolbar</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
    <item name="colorPrimary">#0CECA5</item>
    <item name="colorPrimaryVariant">@color/ic_quiz3_background</item>
    <item name="colorOnPrimary">@color/purple_200</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">@color/teal_200</item>
    <item name="colorSecondaryVariant">@color/ic_quiz3_background</item>
    <item name="colorOnSecondary">#2196F3</item>
    <!-- Status bar color. -->
    <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>

    <!-- Customize your theme here. -->
</style>

    <style name="SS_Theme.C" parent="Theme.MaterialComponents.DayNight.NoActionBar">
